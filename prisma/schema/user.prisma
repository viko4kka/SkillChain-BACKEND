model User {
    id            Int            @id @default(autoincrement())
    linkedinId    String?        @unique
    firstName     String
    lastName      String
    imgUrl        String?
    email         String?
    description   String?
    job           String?
    gitUrl        String?
    linkedinUrl   String?
    userSkills   UserSkill[]
    userLanguages UserLanguage[]
    userLocations UserLocation[]
    projects      Project[]
}
